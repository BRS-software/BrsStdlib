{
    "name": "brs-software/brs-stdlib",
    "homepage": "https://github.com/brssoftware/BrsStdlib",
    "description": "Common general classes",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "brs"
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Brs\\Stdlib\\": "src/",
            "BrsStdlibTest\\": "tests/"
        },
        "files": [
            "functions/GeneralFunctions.php",
            "functions/DebugFunctions.php"
        ],
        "classmap": [
            "./Module.php"
        ]
    }
}
